# Non-recursive Fibonacci
def fib_nonrec(n):
    a, b = 0, 1
    for i in range(n):
        print(a, end=" ")
        a, b = b, a + b

# Recursive Fibonacci
def fib_rec(n):
    if n <= 1:
        return n
    return fib_rec(n-1) + fib_rec(n-2)

n = int(input("Enter n: "))
print("Non-recursive:")
fib_nonrec(n)
print("\nRecursive:")
for i in range(n):
    print(fib_rec(i), end=" ")
