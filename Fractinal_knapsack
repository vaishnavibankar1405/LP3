def fra_knap(p,w,C):
    ratio = [(p[i]/w[i],w[i],p[i]) for i in range(len(p))]
    ratio.sort(reverse=True)

    total=0;
    for r,weight,profit in ratio:
        if C>=weight:
            C-=weight
            total += profit
        else:
            total += r*C
            break
    return total

p=[10,15,7,8,9,4]
w=[1,3,5,4,1,3]
C=15

print("max profit =",fra_knap(p,w,C))
